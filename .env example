DATABASE=your_database_name
DATABASE_HOST=localhost or your_database_host
DATABASE_USER=your_database_user
DATABASE_PASSWORD=your_database_password
JWT_SECRET=your_jwt_secret
JWT_EXPIRES_IN=3600  # Expires in 1 hour, for example
JWT_COOKIE_EXPIRES=90 # Cookie expires in 90 days, for example
PORT=8080
